<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Sequences for sequence carousel slide -->
        <record id="ir_sequence_carousel_slide" model="ir.sequence">
            <field name="name">sequence carousel slide</field>
            <field name="code">gadr.carousel.slide</field>
            <field name="prefix">CSH</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="ir_sequence_gadr_projects" model="ir.sequence">
            <field name="name">sequence Gadr Projects</field>
            <field name="code">gadr.projects.code</field>
            <field name="prefix">GP</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="ir_sequence_gadr_event" model="ir.sequence">
            <field name="name">sequence Gadr event</field>
            <field name="code">gadr.event.code</field>
            <field name="prefix">GE</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>
